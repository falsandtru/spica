{
  "root": true,
  "extends": [
    //"eslint:recommended",
    //"plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": { "project": ["./tsconfig.json"] },
  "plugins": [
    //"@typescript-eslint",
    "redos"
  ],
  "rules": {
    "redos/no-vulnerable": [
      "error",
      {
        "ignoreErrors": false,
        "timeout": 20000
      }
    ]
  },
  "ignorePatterns": [
    "**/*.test.ts"
  ]
}
